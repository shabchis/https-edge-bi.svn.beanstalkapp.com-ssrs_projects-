        </Field>
        <Field Name="ID_Measures___Week_3_">
          <DataField>[Measures].[Week_3]</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ID_Measures___Week_4_">
          <DataField>[Measures].[Week_4]</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ID_Measures___Weight_">
          <DataField>[Measures].[Weight]</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Seperia_DWH</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>declare @Account_ID varchar(20)
declare @p varchar(2)
Set @Account_ID = 7
set @p = 4
-----------------------------
declare @from  varchar(1000)
declare @select varchar(max)
declare @where varchar(max)


declare @Regs  varchar(50)
declare @Actives  varchar(50)
declare @CubeName varchar(1000)
declare @pDate varchar(8)
declare @StartDate1 varchar(8)
declare @EndDate1 varchar(8)
declare @StartDate2 varchar(8)
declare @EndDate2 varchar(8)
declare @StartDate3 varchar(8)
declare @EndDate3 varchar(8)
declare @StartDate4 varchar(8)
declare @EndDate4 varchar(8)



set @pDate = '20130528'
set @StartDate1 = convert(varchar,(select DATEADD(dd, -7, @pDate)) , 112)
set @EndDate1   = @pDate
set @StartDate2 =  convert(varchar,(select DATEADD(dd, -14, @pDate)), 112)
set @EndDate2 =   convert(varchar,(select DATEADD(dd, -7, @pDate)), 112)
set @StartDate3 =  convert(varchar,(select DATEADD(dd, -21, @pDate)), 112)
set @EndDate3 =   convert(varchar,(select DATEADD(dd, -14, @pDate)), 112)
set @StartDate4 =  convert(varchar,(select DATEADD(dd, -28, @pDate)), 112)
set @EndDate4 =   convert(varchar,(select DATEADD(dd, -21, @pDate)), 112)

set @Regs = (select MDX_Name from [Seperia].[dbo].[Measure] where AccountID = @Account_ID and BaseMeasureID = 5)
set @Actives = (select MDX_Name from [Seperia].[dbo].[Measure] where AccountID = @Account_ID and BaseMeasureID = 7)
set @CubeName  = (select substring(convert(varchar(50), AnalysisSettings, 0 ),29, (len( convert(varchar(50), AnalysisSettings, 0 )  ) - 31 )) 
 FROM [Seperia].[dbo].[User_GUI_Account] where Account_ID =  @Account_ID  )
print @CubeName

--print @StartDate1
--print @EndDate1
--print @StartDate2
--print @EndDate2
--print @StartDate3
--print @EndDate3
--print @StartDate4
--print @EndDate4


set @select = 'WITH MEMBER [Measures].[0.1% of Account] AS (([Measures].['+@Regs+'],[Paid Creatives Dim].[Creatives].CurrentMember.Parent.Parent) * 0.001)                     
member [Week_1] AS sum( { [Time Dim].[Time Dim].[Day].&amp;['+@StartDate1+']:[Time Dim].[Time Dim].[Day].&amp;['+@EndDate1+']} , [Measures].[Regs])
member [Week_2] AS sum( { [Time Dim].[Time Dim].[Day].&amp;['+@StartDate2+']:[Time Dim].[Time Dim].[Day].&amp;['+@EndDate2+']} , [Measures].[Regs])
member [Week_3] AS sum( { [Time Dim].[Time Dim].[Day].&amp;['+@StartDate3+']:[Time Dim].[Time Dim].[Day].&amp;['+@EndDate3+']} , [Measures].[Regs])
member [Week_4] AS sum( { [Time Dim].[Time Dim].[Day].&amp;['+@StartDate4+']:[Time Dim].[Time Dim].[Day].&amp;['+@EndDate4+']} , [Measures].[Regs])
SET [Filtered_AdGroups] AS FILTER( {[Paid Creatives Dim].[Creatives].[Ad Group]} , [Measures].['+@Regs+'] &gt; [Measures].[0.1% of Account] 
	                                AND [Week_1] &lt;= [Week_2] 
	                                AND [Week_2] &lt;= [Week_3]
	                                AND [Week_3] &lt;= [Week_4]
	                                AND [Week_1]*'+@p+' &lt;=  ([Week_4])
	                             
	                             )  
	                            
member [Weight] AS  ([Week_4] - [Week_1])

SELECT  NON EMPTY ORDER([Filtered_AdGroups], [Weight], bdesc)   on rows,
       {[Week_1], [Week_2], [Week_3], [Week_4],[Weight]} ON columns 
'
set @from =  'FROM ['+@CubeName+']'
set @where = 

'WHERE ( [Accounts Dim].[Accounts].[Account].&amp;['+@Account_ID+'], [Time Dim].[Time Dim].[Day].&amp;['+@StartDate4+']:[Time Dim].[Time Dim].[Day].&amp;['+@pDate+'] )'
--print @select
--print @from
--print @where
exec Seperia..SP_ExecuteMDX  @select,  @from, @where</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
